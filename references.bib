	urldate = {2024-03-11},
	journal = {Personality and Individual Differences},
	author = {Matthews, Gerald and Hancock, Peter A. and Lin, Jinchao and Panganiban, April Rose and Reinerman-Jones, Lauren E. and Szalma, James L. and Wohleber, Ryan W.},
	month = feb,
