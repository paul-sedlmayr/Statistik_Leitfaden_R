	pages = {1313--1337},
	file = {Gilson et al. - 2015 - Virtual Teams Research 10 Years, 10 Themes, and 1.pdf:/Users/paulsedlmayr/Zotero/storage/63UQXGEL/Gilson et al. - 2015 - Virtual Teams Research 10 Years, 10 Themes, and 1.pdf:application/pdf},
