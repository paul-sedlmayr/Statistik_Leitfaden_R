	author = {Niu, Misaki and Kasai, Atsushi and Tanuma, Masato and Seiriki, Kaoru and Igarashi, Hisato and Kuwaki, Takahiro and Nagayasu, Kazuki and Miyaji, Keita and Ueno, Hiroki and Tanabe, Wataru and Seo, Kei and Yokoyama, Rei and Ohkubo, Jin and Ago, Yukio and Hayashida, Misuzu and Inoue, Ken-ichi and Takada, Masahiko and Yamaguchi, Shun and Nakazawa, Takanobu and Kaneko, Shuji and Okuno, Hiroyuki and Yamanaka, Akihiro and Hashimoto, Hitoshi},
	month = mar,
