	journal = {Journal of Anxiety Disorders},
	author = {Carl, Emily and Stein, Aliza T. and Levihn-Coon, Andrew and Pogue, Jamie R. and Rothbaum, Barbara and Emmelkamp, Paul and Asmundson, Gordon J.G. and Carlbring, Per and Powers, Mark B.},
	month = jan,
	year = {2019},
